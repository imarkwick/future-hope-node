    <body>
        <h2 id="thankyou">Thanks To : </h2>
        <div id="display-names">
            <marquee behavior="scroll"  id="disp" direction="left"> </marquee>
        </div>

        <div id="wholepage">
        </div>
        <section id="top-space"></section>
        <div id="all-mela">
            <div id="database-images">
                <% if (items) { %>
                    <% items.forEach(function(image) { %>
                        <img class="wow bounceIn" height="120px" width="120px" src="images/<%= image %>" />
                    <% }); %>
                <% }; %>
            </div>
            <section id="socketimgs">
            <div id="future-hope-photos">
                <img class="futurehope" id="photo1" src="/images/cricketoneyear.png">
                <img class="futurehope" id="photo2" src="/images/teacheroneyear.png">
                <img class="futurehope" id="photo3" src="/images/education6months.png">
                <img class="futurehope" id="photo4" src="/images/activitiesbig.png">
                <img class="futurehope" id="photo5" src="/images/rugbylarge.png">
                <img class="futurehope" id="photo6" src="/images/homeandeducation.png">
                <img class="futurehope" id="photo7" src="/images/houseparents.png">
                <img class="futurehope" id="photo8" src="/images/degree.png">
            </div>
                <div class="socket-images" id="display-rugby-messages"></div>
                <div class="socket-images" id="display-cricket-messages"></div>
                <div class="socket-images" id="display-messages"></div>
            </section>
        </div>

        <script src="javascripts/jquery.js"></script>
        <script src="javascripts/foundation.js"></script>
        <script src="javascripts/script.js"></script>
        <script src="/socket.io/socket.io.js"></script>
        <script src="javascripts/wow.min.js"></script>
        <script src="javascripts/display.js"></script>
    </body>
</html>